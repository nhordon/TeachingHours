<%- include("../partials/header"); %>
<div class="col-12">
    <h1>Specialities</h1>
    <a href="/specialities/add" class="btn btn-primary mb-2 btn-sm">Додати</a>
    <table class="table table-bordered table-striped table-sm">
        <thead>
            <tr>
                <th width="90px">Дії</th>
               
                <th>number</th>
                <th>abr</th>
                <th>name</th>
                
            </tr>
        </thead>
        <tbody>
            <% specialities.forEach(specialities => { %>
            <tr>
                <td>
                    <a href="/specialities/edit/<%= specialities._id %>" class="btn btn-warning btn-sm" title="Редагувати">
                        <img src="images/edit-24px.svg" width="16px" height="16px" />                   
                    </a>             
                   
                    <a href="/specialities/delete/<%= specialities._id %>" class="btn btn-danger btn-sm" title="Видалити">
                        <img src="images/delete_outline-24px.svg" width="16px" height="16px" />
                    </a>
                </td>
                <td><%= specialities.number %></td>
                <td><%= specialities.abr %></td>
                <td><%= specialities.name %></td>
               
            </tr>
            <% })%>
        </tbody>
    </table>
</div>
<%- include("../partials/footer"); %>